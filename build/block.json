{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "emt/section-block",
  "version": "1.0",
  "title": "EMT Section",
  "category": "widgets",
  "icon": "smiley",
  "description": "This is a section",
  "supports": {
    "html": false,
    "align": [
      "full"
    ],
    "color": {
      "background": true
    }
  },
  "textdomain": "section-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "padding": {
      "type": "string",
      "default": "0"
    },
    "align": {
      "type": "string",
      "default": "full"
    },
    "backgroundColor": {
      "type": "string",
      "default": "transparent"
    }
  }
}